<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/login.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.project_healtheducation.controllers.LoginController">
    <children>
        <VBox fx:id="loginContainer" alignment="CENTER" prefHeight="473.0" prefWidth="705.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

             <Text fx:id="text" fill="#41bc18" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">Login</Text>

            <VBox alignment="CENTER">
                <VBox alignment="CENTER" prefHeight="94.0" prefWidth="600.0" spacing="5">
                        <Text>Email:</Text>
                    <HBox alignment="CENTER" prefHeight="26.0" prefWidth="589.0" spacing="10">
                        <TextField fx:id="textNome" />
                    </HBox>
                        <Text>Senha:</Text>
                    <HBox alignment="CENTER" spacing="10">
                        <PasswordField fx:id="textSenha" />
                    </HBox>
                </VBox>
            </VBox>
          <Button onAction="#irParaHome" prefHeight="26.0" prefWidth="102.0" styleClass="btnLogin">Entrar</Button>
            </VBox>

              <Button onAction="#voltarInicio" styleClass="btnVoltar" textAlignment="CENTER" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">Voltar</Button>

    </children>
</AnchorPane>
